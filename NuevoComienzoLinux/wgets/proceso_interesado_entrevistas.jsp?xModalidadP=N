        function Ocultar(valor) {
            var tipo_otro = document.getElementById("yCveTipoSeguroOtro").value;
            if (valor == tipo_otro) {
                document.getElementById('xSeguroSocial').style.display = '';
                document.getElementById('SeguroSocial').style.display = '';
