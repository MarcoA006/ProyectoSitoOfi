		function valida_curp(){
		var cup = Trim(document.forma.xCurp.value);
		//alert(cup);
			if(cup != "" && cup.length==18){
				document.forma.yAccion.value = "VCURP";
				document.forma.submit();
			}else{
				alert("debe proporcionar un curp válida");
				document.forma.xCurp.value = "-";
				}
		}
		
